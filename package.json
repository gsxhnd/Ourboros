  "type": "module",
  "scripts": {
    "dev:electron": "node script/dev.electron.js",
    "dev:renderer": "vite",
    "dev": "yarn dev:electron & yarn dev:renderer",
    "dev:napi": "napi build --cargo-name napi ./electron/napi",
