    "lowdb": "^7.0.1",
    "node-sqlite3-wasm": "^0.7.1",
    "pinia": "^2.1.7",
    "primevue": "^3.52.0",
    "vue": "^3.4.21",
    "vue-router": "^4.3.2"
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.4.0",
    "@electron/packager": "^18.3.2",
    "@napi-rs/cli": "^2.18.3",
    "@types/node": "^20.12.12",
    "@types/nodemon": "^1.19.6",
    "@vitejs/plugin-vue": "^5.0.4",
    "dotenv": "^16.4.5",
    "dotenv-cli": "^7.4.2",
